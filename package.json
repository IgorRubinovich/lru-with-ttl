{
  "name": "lru-with-ttl",
  "version": "1.0.1",
  "description": "A Time-aware LRU key--value store with global/per value ttl setting and efficient lookup",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --external:events --external:fast-deep-equal && tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "test": "node test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorRubinovich/lru-with-ttl.git"
  },
  "keywords": [
    "lru",
    "cache",
    "ttl"
  ],
  "author": "Igor Rubinovich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IgorRubinovich/lru-with-ttl/issues"
  },
  "homepage": "https://github.com/IgorRubinovich/lru-with-ttl#readme",
  "dependencies": {
    "fast-deep-equal": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "esbuild": "^0.27.2",
    "jest": "^30.2.0",
    "typescript": "^5.9.3"
  }
}
